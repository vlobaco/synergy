<?xml version="1.0" encoding="UTF-8"?> 
<entity-mappings 
	xmlns="http://java.sun.com/xml/ns/persistence/orm" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd" 
	version="1.0"
	> 
    <entity name="NumberField" class="synergy.api.NumberField" access="FIELD">
    	<discriminator-value>number</discriminator-value>
        <attributes>
            <basic name="number" optional="true"> 
                <column name="NUMBER"/> 
            </basic>
            <basic name="min" optional="true">
            	<column name="MIN"/>
            </basic>
            <basic name="max" optional="true">
            	<column name="MAX"/>
            </basic>
        </attributes>
     </entity> 
</entity-mappings>